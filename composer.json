{
  "name": "wearesho-team/yii2-google-autocomplete",
  "description": "Yii2 google-api integration",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">7.1",
    "wearesho-team/google-autocomplete": "^2.1",
    "yiisoft/yii2": "^2.0.15",
    "wearesho-team/yii-http": "^1.12"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "authors": [
    {
      "name": "Roman Varkuta",
      "email": "roman.varkuta@gmail.com"
    },
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wearesho\\GoogleAutocomplete\\Yii\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wearesho\\GoogleAutocomplete\\Yii\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
